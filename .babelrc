{
  "presets": ["es2015", "react", "stage-0"],
  "plugins": ["transform-runtime"],
  "env": {
    "development": {
      "presets": [
        [
          "es2015",
          {
            "modules": false
          }
        ],
        "react",
        "stage-0"
      ],
      "plugins": ["react-hot-loader/babel"]
    },
    "production": {
      "presets": ["es2015", "react", "stage-0"],
      "plugins": [
        "transform-react-constant-elements",
        "transform-react-inline-elements",
        "add-module-exports"
      ]
    }
  }
}
