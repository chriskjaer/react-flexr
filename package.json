{
  "name": "react-flexr",
  "version": "3.0.0-beta.1",
  "description": "React flexbox grid made simple.",
  "author": "Kodyl ApS",
  "contributors": [
    {
      "name": "Chris KjÃ¦r SÃ¸rensen"
